# Ignore configuration files that may contain sensitive information.
docroot/sites/*/settings*.php

# Ignore paths that contain user-generated content.
docroot/sites/*/files
docroot/sites/*/private

# Ignore project files generated by eclipse
.project
.settings
.settings/*
.buildpath

# Ignore files added by vim with ~ at the end
*~

# Ignore swap files (generated by some of the text editors)
*.swp
one
two

#Ignore cache generated by Sass
.sass-cache
.sass-cache/*

#Ignore .DS_Store
*.DS_Store

#Ignore hcltech Drupal 6 unnecessary files
docroot/.index.php.swp
docroot/.orig_druphtaccess
docroot/.svn/
docroot/.error_log

# Ignore files generated from applying/reverting patches
*.rej
*.orig
